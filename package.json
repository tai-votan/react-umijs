{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "umi": "3.5.23"
  },
  "devDependencies": {
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@umijs/fabric": "2.10.2",
    "@umijs/preset-react": "2.1.2",
    "@umijs/test": "3.5.23",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "typescript": "4.6.3",
    "yorkie": "2.0.0"
  }
}
