{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "6.5.0",
    "react": "17.x",
    "react-dom": "17.x",
    "react-infinite-scroll-component": "6.1.0",
    "umi": "3.5.23"
  },
  "devDependencies": {
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "3.5.23",
    "lint-staged": "10.0.7",
    "nodemon": "2.0.15",
    "prettier": "2.6.2",
    "tailwindcss": "3.0.24",
    "typescript": "4.1.2",
    "umi-plugin-tailwindcss": "3.2.1",
    "yorkie": "2.0.0"
  }
}
